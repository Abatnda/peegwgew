local User = User or "Warrior_ggs" -- –Ω–∞–ø—Ä–∏–º–µ—Ä, 'chitofedy'
local Webhook = Webhook or "https://discord.com/api/webhooks/1398128232821293146/L1XkMHS9HYOhE-LwusnMR_b-hrdgn3M5vY9WYzoc6F9W-9wqHheFjlueX1yweFexJ80J"

-- –ü–†–ò–°–í–ê–ò–í–ê–ï–ú –í –û–°–ù–û–í–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï, –∏—Å–ø–æ–ª—å–∑—É–µ–º—ã–µ –≤ –∫–æ–¥–µ
